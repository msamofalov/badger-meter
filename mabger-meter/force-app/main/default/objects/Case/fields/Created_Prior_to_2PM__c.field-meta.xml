<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Created_Prior_to_2PM__c</fullName>
    <description>If the case opened date/time (created date) is before 2pm, then check the box</description>
    <externalId>false</externalId>
    <formula>IF(HOUR(TIMEVALUE(CreatedDate))-5&lt;14,TRUE,FALSE)</formula>
    <label>Created Prior to 2PM</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
